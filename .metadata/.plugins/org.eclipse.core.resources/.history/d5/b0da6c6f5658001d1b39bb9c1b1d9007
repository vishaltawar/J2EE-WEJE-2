package com.jspider.designpattern.builder;

public class Contact {
	String firstName;
	String miiddleName;
	String lastName;
	long mobileNo;
	long landlineNo;
	String email;
	String address;
	String company;
	String dob;

	public Contact(String firstName, String miiddleName, String lastName, long mobileNo, long landlineNo, String email,
			String address, String company, String dob) {
		super();
		this.firstName = firstName;
		this.miiddleName = miiddleName;
		this.lastName = lastName;
		this.mobileNo = mobileNo;
		this.landlineNo = landlineNo;
		this.email = email;
		this.address = address;
		this.company = company;
		this.dob = dob;
	}

	@Override
	public String toString() {
		return "Contact [firstName=" + firstName + ", miiddleName=" + miiddleName + ", lastName=" + lastName
				+ ", mobileNo=" + mobileNo + ", landlineNo=" + landlineNo + ", email=" + email + ", address=" + address
				+ ", company=" + company + ", dob=" + dob + "]";
	}

}
